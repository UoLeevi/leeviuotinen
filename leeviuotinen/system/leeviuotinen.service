[Unit]
Description=ASP.NET Core App for leeviuotinen.com running on Ubuntu

[Service]
WorkingDirectory=/var/aspnetcore/leeviuotinen-app
ExecStart=/usr/bin/dotnet /var/aspnetcore/leeviuotinen-app/leeviuotinen.dll
Restart=always
RestartSec=10
SyslogIdentifier=dotnet-leeviuotinen
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target